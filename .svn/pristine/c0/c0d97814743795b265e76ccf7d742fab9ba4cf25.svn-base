/*End*/


article, aside, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section {
	display: block;
}
body {
	line-height: 1;
}
ol, ul {
	list-style: none;
}
/*change*/
ol {
    margin: 0 0 0px;
}
ol.grid {
background: transparent;
    -webkit-border-radius: 12px;
    -moz-border-radius:    12px;
    border-radius:         12px;

    /*-webkit-box-shadow: 0 0 10px rgba(0,0,0,0.153);*/

   color: #0464BB;
    display: block;
/*    font-size: 16px;*/
    height:105px;
/*    line-height: 10px;*/
    padding: 0;
    text-align: center;
    text-decoration: none;
    text-shadow: 0 1px 0 rgba(255,255,255,0.333);
    vertical-align: middle;
    width:290px;
    overflow: auto; margin:0px;

}

/*change*/

blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}

/******* END STYLE RESET *******/

body {
	width: 320px;
	font-family: Arial, Helvetica, sans-serif;
	background-color: #e1e1e1;
}

.clear {
	clear: both;
}

#header {
	/*padding-bottom: 15px;*/
	
}

h1 {
	font-size: larger;
	color: #333333;
	padding-bottom: .5em;
}
p.description {
	font-size: small;
	font-weight: normal;
	color: #616161;
}
.title {
	margin: 0 20px;
}

#nav {
	margin: 0; padding: 0;
}
#nav div.container-top {
	background: url(../images/container_top.png) no-repeat top center;
	width: 100%; height: 60px;
}
#nav div {
	float: left;
	width: 33%;
	height: 110px;
}
#nav div img {
	width: 50px;
	margin: 0 25px;
	padding: 0;
}
#nav div a {
	font-size: 11px;
	text-decoration: none;
	color: #58595d;
}

#nav p {
	width: 110px;
	margin: 0 auto;
	text-align: center;
	padding: 0;
	color: #58595d;
}

