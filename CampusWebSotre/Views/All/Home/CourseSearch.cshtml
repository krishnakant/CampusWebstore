                $("#processing").html('<img src="@Url.Content("~/Content/images/Desktop/loading_normal_up.gif")"> Please Wait...<br>');
                var termId = document.getElementById('ddlTerm').value;

                $.ajax({
                    type: "POST",
                    url: '@Url.Action("GetDepartments", "Home")',
