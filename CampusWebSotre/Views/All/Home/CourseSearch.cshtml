        /**
        * ddlDepartment change event
        */
        $("#ddlDepartment").change(function () {
            clearContentArea();
            $("#ddlDepartment").focus();

            var storeId = $('#ddlStore').val();
            var termId = $('#ddlTerm').val();
            var deptId = $('#ddlDepartment').val();

