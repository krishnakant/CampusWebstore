                        //                        strTable+='<a data-transition="slideup" data-rel="dialog" href=' + "'" + 'javascript:fnGetTax("' + data.ShippingMethods[i].Display + '","' + data.ShippingMethods[i].Amount + '","' + data.ShippingMethods[i].Table + '");' + "'" + ' title="Purchase album'+
